{
  "name": "ceviche",
  "amdName": "ceviche",
  "version": "0.1.0",
  "description": "Something something virtual DOM.",
  "source": "src/index.js",
  "module": "dist/ceviche.es.js",
  "main": "dist/ceviche.js",
  "umd:main": "dist/ceviche.umd.js",
  "scripts": {
    "build": "microbundle build",
    "dev": "microbundle watch --format cjs,umd",
    "test": "eslint src"
  },
  "mangle": {
    "regex": "^(_)"
  },
  "eslintConfig": {
    "extends": "eslint-config-developit",
    "settings": {
      "react": {
        "pragma": "createElement"
      }
    },
    "rules": {
      "prefer-rest-params": 0,
      "prefer-spread": 0,
      "no-cond-assign": 0
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "ceviche",
    "Virtual DOM",
    "VDOM",
    "DOM"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "repository": "developit/ceviche",
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-developit": "^1.1.1",
    "jest": "^22.1.4",
    "microbundle": "^0.4.1"
  },
  "dependencies": {
    "object-shape": "^0.2.0"
  }
}
